<?xml version="1.0" encoding="UTF-8"?>
<setup:Project
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:git="http://www.eclipse.org/oomph/setup/git/1.0"
    xmlns:mylyn="http://www.eclipse.org/oomph/setup/mylyn/1.0"
    xmlns:pde="http://www.eclipse.org/oomph/setup/pde/1.0"
    xmlns:predicates="http://www.eclipse.org/oomph/predicates/1.0"
    xmlns:projects="http://www.eclipse.org/oomph/setup/projects/1.0"
    xmlns:setup="http://www.eclipse.org/oomph/setup/1.0"
    xmlns:setup.targlets="http://www.eclipse.org/oomph/setup/targlets/1.0"
    xmlns:setup.workingsets="http://www.eclipse.org/oomph/setup/workingsets/1.0"
    xsi:schemaLocation="http://www.eclipse.org/oomph/setup/git/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/Git.ecore http://www.eclipse.org/oomph/setup/mylyn/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/Mylyn.ecore http://www.eclipse.org/oomph/setup/pde/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/PDE.ecore http://www.eclipse.org/oomph/predicates/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/Predicates.ecore http://www.eclipse.org/oomph/setup/projects/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/Projects.ecore http://www.eclipse.org/oomph/setup/targlets/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/SetupTarglets.ecore http://www.eclipse.org/oomph/setup/workingsets/1.0 http://git.eclipse.org/c/oomph/org.eclipse.oomph.git/plain/setups/models/SetupWorkingSets.ecore"
    name="org.glassfish.jersey"
    label="Glassfish Jersey">
  <setupTask
      xsi:type="pde:APIBaselineTask"
      name="ECF"
      version="3.13.2"
      location="${workspace.location/../api.baseline}"
      remoteURI="http://download.eclipse.org/rt/ecf/3.13.2/org.eclipse.ecf.sdk_3.13.2.v20160823-2221.zip"/>
  <setupTask
      xsi:type="git:GitCloneTask"
      id="org.glassfish.jersey.git.clone"
      remoteURI="agentlab/org.glassfish.jersey">
    <annotation
        source="http://www.eclipse.org/oomph/setup/InducedChoices">
      <detail
          key="inherit">
        <value>github.remoteURIs</value>
      </detail>
      <detail
          key="label">
        <value>${scope.project.label} Github repository</value>
      </detail>
      <detail
          key="target">
        <value>remoteURI</value>
      </detail>
    </annotation>
    <description>${scope.project.label}</description>
  </setupTask>
  <setupTask
      xsi:type="setup.targlets:TargletTask">
    <targlet
        name="${scope.project.label}"
        activeRepositoryList="${eclipse.target.platform}">
      <annotation
          source="http:/www.eclipse.org/oomph/targlets/TargetDefinitionGenerator">
        <detail
            key="location">
          <value>${org.glassfish.jersey.git.clone.location/releng/org.glassfish.jersey.target/org.glassfish.jersey.target.target}</value>
        </detail>
        <detail
            key="includeAllPlatforms">
          <value>true</value>
        </detail>
        <detail
            key="includeSource">
          <value>true</value>
        </detail>
        <detail
            key="generateVersions">
          <value>false</value>
        </detail>
        <detail
            key="generateImplicitUnits">
          <value>true</value>
        </detail>
        <detail
            key="includeMode">
          <value>slicer</value>
        </detail>
      </annotation>
      <requirement
          name="*"/>
      <requirement
          name="com.fasterxml.jackson.core.jackson-annotations"/>
      <requirement
          name="com.fasterxml.jackson.core.jackson-core"/>
      <requirement
          name="com.fasterxml.jackson.core.jackson-databind"/>
      <requirement
          name="com.fasterxml.jackson.datatype.jackson-datatype-guava"/>
      <requirement
          name="com.fasterxml.jackson.jaxrs.jackson-jaxrs-base"/>
      <requirement
          name="com.fasterxml.jackson.jaxrs.jackson-jaxrs-json-provider"/>
      <requirement
          name="com.fasterxml.jackson.module.jackson-module-jaxb-annotations"/>
      <requirement
          name="javassist"/>
      <requirement
          name="javax.annotation"/>
      <requirement
          name="javax.inject"/>
      <requirement
          name="javax.persistence"/>
      <requirement
          name="javax.validation"/>
      <requirement
          name="javax.ws.rs"
          versionRange="[2.0.1,2.1.0)"/>
      <requirement
          name="org.aopalliance"/>
      <requirement
          name="org.apache.aries.spifly.dynamic.bundle"/>
      <requirement
          name="org.apache.aries.util"/>
      <requirement
          name="org.eclipse.core.runtime.feature.feature.group"/>
      <requirement
          name="org.eclipse.ecf.core.feature.group"/>
      <requirement
          name="org.eclipse.equinox.http.jetty"/>
      <requirement
          name="org.eclipse.equinox.http.servlet"/>
      <requirement
          name="org.eclipse.equinox.util"/>
      <requirement
          name="org.eclipse.jetty.continuation"/>
      <requirement
          name="org.eclipse.jetty.http"/>
      <requirement
          name="org.eclipse.jetty.io"/>
      <requirement
          name="org.eclipse.jetty.security"/>
      <requirement
          name="org.eclipse.jetty.server"/>
      <requirement
          name="org.eclipse.jetty.servlet"/>
      <requirement
          name="org.eclipse.jetty.util"/>
      <requirement
          name="org.eclipse.jetty.websocket.api"/>
      <requirement
          name="org.eclipse.jetty.websocket.client"/>
      <requirement
          name="org.eclipse.jetty.websocket.common"/>
      <requirement
          name="org.eclipse.jetty.websocket.server"/>
      <requirement
          name="org.eclipse.jetty.websocket.servlet"/>
      <requirement
          name="org.glassfish.hk2.api"/>
      <requirement
          name="org.glassfish.hk2.locator"/>
      <requirement
          name="org.glassfish.hk2.osgi-resource-locator"
          versionRange="[1.0.0,2.0.0)"/>
      <requirement
          name="org.glassfish.hk2.utils"/>
      <requirement
          name="org.objectweb.asm"/>
      <requirement
          name="javax.validation.api"/>
      <requirement
          name="org.json"/>
      <requirement
          name="org.eclipse.core.net"/>
      <requirement
          name="org.apache.httpcomponents.httpclient"
          versionRange="4.3.6.v201511171540"/>
      <sourceLocator
          rootFolder="${org.glassfish.jersey.git.clone.location}"
          locateNestedProjects="true"/>
      <repositoryList
          name="Neon">
        <repository
            url="http://download.eclipse.org/rt/ecf/latest/site.p2"/>
        <repository
            url="http://download.eclipse.org/jetty/updates/jetty-bundles-9.x/9.3.10.v20160621"/>
        <repository
            url="http://agentlab.ru/pd/p2/che/"/>
        <repository
            url="http://download.eclipse.org/releases/neon/201610111000"/>
      </repositoryList>
    </targlet>
  </setupTask>
  <setupTask
      xsi:type="setup.workingsets:WorkingSetTask">
    <workingSet
        name="Jersey"
        id="">
      <predicate
          xsi:type="predicates:LocationPredicate"
          pattern=".*/org.glassfish.jersey/.*"/>
    </workingSet>
  </setupTask>
  <setupTask
      xsi:type="mylyn:MylynQueriesTask"
      connectorKind="github"
      repositoryURL="https://github.com/agentlab/org.glassfish.jersey"
      userID="${github.user.id}"
      password="${github.user.password}">
    <query summary="${scope.project.label} All Open">
      <attribute
          key="state"
          value="open::"/>
    </query>
    <query summary="${scope.project.label} My Open">
      <attribute
          key="state"
          value="open::"/>
      <attribute
          key="assignee"
          value="${github.user.id}"/>
    </query>
    <query summary="${scope.project.label} My Closed"
        url="">
      <attribute
          key="state"
          value="closed::"/>
      <attribute
          key="assignee"
          value="${github.user.id}"/>
    </query>
  </setupTask>
  <setupTask
      xsi:type="projects:ProjectsBuildTask"
      id="InitialBuild"
      refresh="true">
    <description>Initial Build</description>
  </setupTask>
  <stream name="master"/>
  <logicalProjectContainer
      xsi:type="setup:Project"
      href="index:/org.eclipse.setup#//@projectCatalogs[name='ru.agentlab']/@projects[name='osgi']"/>
  <description>Glassfish Jersey WebServices</description>
</setup:Project>
